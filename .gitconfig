[alias]
    l = log --pretty=oneline -n 20 --graph --abbrev-commit
    s = status
    c = commit
    d = diff
    br = branch
    ce = checkout
    ds = diff --staged
    last = log -1
    logp = log --graph --oneline --pretty
    logone = log --oneline
    pullr = pull --rebase
    uncommit = reset HEAD^
    branches = branch -a
    remotes = remote -v

[merge]
    log = true

[push]
    default = matching

[user]
    name = Ivaylo Tsankov
    email = itsankoff@gmail.com

[url "git@github.com:"]
    insteadOf = https://github.com/

[url "git@bitbucket.org:"]
    insteadOf = https://bitbucket.org/

[pager]
    branch = false
